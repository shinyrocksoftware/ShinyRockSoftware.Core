{
    "Service": {
        "ClientName": "Lab",
        "Code": "lab-app-grpc-mysql",
        "Name": "Lab Grpc MySql App",
        "Version": "1.0.0"
    },
    "Serilog": {
        "MinimumLevel": {
            "Default": "Warning"
        },
        "WriteTo": [
            {
                "Name": "Console",
                "Args": {
                    "outputTemplate": "===> {Timestamp:HH:mm:ss.fff zzz} [{Level}] {Message}{NewLine}{Exception}"
                }
            }
        ]
    }
}